<div id="home">
	<div class="container">
		<div class="col-8 offset-2">
			<div class="animated bounceInDown">
				<br>
				<span class="app_title"><a href="/" class="no-underline">
						<span class="tama tama_fix">Tama</span><span class="fit">Fit</span>
					</a></span>
			</div>
			<hr class="type_6 animated bounceInDown hide-rule">
		</div>
	</div>

	<div class="animated bounceInUp">
		<h1 class="h1-mobile-responsive">Log a workout for <span class="member-name"></span></h1>
		<br>
	</div>

	<div class="container hidden">
		<div class="row top-30">
			<div class="col text-center">
				<a class="btn btn-light body-buttons" href="/members">Go to Logs</a>
			</div>
		</div>
		<br>
		<div class="row">
			<div class="col-md-6 offset-md-3">
				<form id="logger">

					<div class="form-group">
						<label for="example-date-input">Workout Date:</label>
						<input class="date" type="date" value="2011-08-19" id="datePicker">
						{{!-- <input type="date" id="date" name="trip-start" value="2019-01-24" min="2018-01-01" max="2025-12-31"> --}}
					</div>

					<div class="form-group">
						<label for="comment">Comment:</label>
						<textarea placeholder="Log Workout" class="form-control" rows="10" id="comment"></textarea>
					</div>

					<div class="form-group">
						<label for="category">Select User:</label>
						<select class="custom-select" id="user">
						</select>
					</div>

					<br>id js: <span class="member-id"></span>
					<br>id hb: {{id}}
					<br>id hb w class: <span class="login-id">{{id}}</span>
					<br>
					<button type="submit" class="btn btn-success submit">Submit</button>

				</form>
			</div>
		</div>
	</div>

</div>
<script src="https://code.jquery.com/jquery.js"></script>
